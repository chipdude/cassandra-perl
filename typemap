TYPEMAP
XClient *		T_PTROBJ
AccessLevel		T_IV
ConsistencyLevel	T_IV
string			T_CPPSTRING
AuthenticationRequest	T_THRIFT
ColumnPath		T_THRIFT
ColumnOrSuperColumn	T_THRIFT

INPUT
T_CPPSTRING
    assign_string($var, $arg)
T_THRIFT
    assign_thrift($var, $arg)

OUTPUT
T_CPPSTRING
    sv_setpvn_mg($arg, $var.data(), $var.size());
T_THRIFT
    sv_setsv($arg, sv_2mortal(output_thrift($var)));
